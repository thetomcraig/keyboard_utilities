# Place the extended scancodes at 0x80 + second byte.

# S0xE0,11:U"RAlt"; # R ALT	E0,F0,11
S0x91:U"RAlt"; # R ALT	E0,F0,11

# S0xE0,14:U"RCtrl"; # R CTRL	E0,F0,14
S0x94:U"RCtrl"; # R CTRL	E0,F0,14

# S0xE0,1F:U"LGui"; # L GUI	E0,F0,1F
S0x9F:U"LGui"; # L GUI	E0,F0,1F

# S0xE0,27:U"RGui"; # R GUI	E0,F0,27
S0xa7:U"RGui"; # R GUI	E0,F0,27

# S0xE0,2F:U""; # APPS	E0,F0,2F
S0x2F:U"App"; # APPS	E0,F0,2F

# S0xE0,4A:U"P/"; # KP /	E0,F0,4A
S0xCA:U"P/"; # KP /	E0,F0,4A

# S0xE0,5A:U"PEnter"; # KP EN	E0,F0,5A
S0xDA:U"PEnter"; # KP EN	E0,F0,5A

# S0xE0,69:U"End"; # END	E0,F0,69
S0xE9:U"End"; # END	E0,F0,69

# S0xE0,6B:U"Left"; # L ARROW	E0,F0,6B
S0xEB:U"Left"; # L ARROW	E0,F0,6B

# S0xE0,6C:U"Home"; # HOME	E0,F0,6C
S0xEC:U"Home"; # HOME	E0,F0,6C

# S0xE0,70:U"Insert"; # INSERT	E0,F0,70
S0xF0:U"Insert"; # INSERT	E0,F0,70

# S0xE0,71:U"Delete"; # DELETE	E0,F0,71
S0xF1:U"Delete"; # DELETE	E0,F0,71

# S0xE0,72:U"Down"; # D ARROW	E0,F0,72
S0xF2:U"Down"; # D ARROW	E0,F0,72

# S0xE0,74:U"Right"; # R ARROW	E0,F0,74
