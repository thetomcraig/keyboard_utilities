{
  "title": "Emacs Readline",
  "author": "",
  "homepage": "",
  "hostpage": "",
  "manual": "",
  "import_url": "",
  "rules": [
    {
      "description": "Hyper word-wise movement",
      "manipulators": [
        {
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": [
                "right_command",
                "right_shift",
                "right_option"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": [
                "left_option"
              ]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "o",
            "modifiers": {
              "mandatory": [
                "right_command",
                "right_shift",
                "right_option"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": [
                "left_option"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
