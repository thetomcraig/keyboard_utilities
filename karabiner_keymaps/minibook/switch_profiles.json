                "rules": [
                    {
                        "description": "CMD+OPT+CTL + D -> Disable Keyboard",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "d",
                                    "modifiers": {
                                        "mandatory": [
                                            "left_command",
                                            "left_option",
                                            "left_control"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "'/Library/Application Support/org.pqrs/Karabiner-Elements/bin/karabiner_cli' --select-profile 'Disable Keys'"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    }
                ]
            },
